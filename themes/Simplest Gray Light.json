    "tokenColors": [
        {
            "scope": [
                "string",
                "constant.numeric",
                "constant.character",
                "constant.other",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#15803d"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.operator.new",
                "storage.modifier",
                "storage.type",
                "constant.language",
                "support.type",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#1d4ed8"
            }
        },
        {
            "scope": [
                "meta",
                "support.type",
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#7e22ce"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#111827"
            }
        },
        {
            "scope": [
                "variable.other.constant"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation",
                "meta.brace",
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#6b7280"
            }
        }
    ]
}