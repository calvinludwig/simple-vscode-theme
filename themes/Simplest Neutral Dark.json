    "tokenColors": [
        {
            "scope": [
                "string",
                "constant.numeric",
                "constant.character",
                "constant.other",
                "punctuation.definition.string"
            ],
            "settings": {
                "foreground": "#86efac"
            }
        },
        {
            "scope": [
                "keyword",
                "keyword.operator.new",
                "storage.modifier",
                "storage.type",
                "constant.language",
                "support.type",
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#93c5fd"
            }
        },
        {
            "scope": [
                "meta",
                "support.type",
                "variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#d8b4fe"
            }
        },
        {
            "scope": [
                "variable"
            ],
            "settings": {
                "foreground": "#f5f5f5"
            }
        },
        {
            "scope": [
                "variable.other.constant"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation",
                "meta.brace",
                "storage.type.function.arrow"
            ],
            "settings": {
                "foreground": "#737373"
            }
        }
    ]
}