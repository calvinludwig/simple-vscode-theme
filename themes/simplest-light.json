	"tokenColors": [
		{
			"scope": [
				"string",
				"constant.numeric",
				"constant.character",
				"constant.other",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#065f46"
			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage.modifier",
				"storage.type",
				"constant.language",
				"support.type",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#1e40af"
			}
		},
		{
			"scope": [
				"meta",
				"support.type",
				"variable.other.readwrite.alias"
			],
			"settings": {
				"foreground": "#6b21a8"
			}
		},
		{
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#171717"
			}
		},
		{
			"scope": [
				"variable.other.constant"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation",
				"meta.brace",
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#737373"
			}
		}
	]
}
